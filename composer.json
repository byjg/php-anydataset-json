{
  "name": "byjg/anydataset-json",
  "description": "Anydataset JSON abstraction. Anydataset is an agnostic data source abstraction layer in PHP.",
  "autoload": {
    "psr-4": {
      "ByJG\\AnyDataset\\Json\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "byjg/anydataset": "4.9.*"
  },
  "suggest": {
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*|7.4.*|^9.6"
  },
  "license": "MIT"
}
