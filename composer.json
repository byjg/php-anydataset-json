{
  "name": "byjg/anydataset-json",
  "description": "Anydataset JSON abstraction. Anydataset is an agnostic data source abstraction layer in PHP.",
  "autoload": {
    "psr-4": {
      "ByJG\\AnyDataset\\Json\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1 <8.4",
    "ext-json": "*",
    "byjg/anydataset": "^5.0"
  },
  "suggest": {
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "vimeo/psalm": "^6.0"
  },
  "provide": {
    "byjg/anydataset-implementation": "1.0"
  },
  "license": "MIT"
}
